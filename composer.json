{
  "name": "towoju/one2one-calls",
  "description": "One-on-one video/audio call scaffolding for Laravel (WebRTC signaling + permissions + events).",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Emmanuel A Towoju",
      "email": "dev@example.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^10.0|^11.0",
    "illuminate/events": "^10.0|^11.0",
    "illuminate/broadcasting": "^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "Towoju\\One2OneCalls\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Towoju\\One2OneCalls\\Providers\\OneToOneCallServiceProvider"
      ]
    }
  }
}